{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/inventory$", "dest": "api/inventory.js" },
    { "src": "/api/inventory/public$", "dest": "api/inventory-public.js" },
    { "src": "/api/inventory/(.*)$", "dest": "api/inventory-id.js" }
  ]
}